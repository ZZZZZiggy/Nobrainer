{
  "extends": "next/core-web-vitals",
  "ignorePatterns": [
    "src/generated/**/*",
    "prisma/migrations/**/*",
    "*.wasm.js",
    "**/wasm*.js",
    "**/runtime/**/*",
    "node_modules/**/*",
    ".next/**/*",
    "out/**/*"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "@typescript-eslint/no-unused-expressions": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  }
}
